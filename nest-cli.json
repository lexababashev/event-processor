{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "monorepo": true,
  "root": "apps",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src"
    },
    "collectors": {
      "type": "application",
      "root": "apps/collectors",
      "entryFile": "main",
      "sourceRoot": "apps/collectors/src"
    },
    "reporter": {
      "type": "application",
      "root": "apps/reporter",
      "entryFile": "main",
      "sourceRoot": "apps/reporter/src"
    }
  }
}